  "scripts": {
    "start": "webpack-dev-server",
    "bundle": "cp src/index.html dist/ & webpack -p ./dist/bundle.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morsdyce/ngReact-minesweeper"
  },
  "author": "Maayan Glikser <maayan@glikm.com>",
  "license": "MIT",
  "homepage": "https://github.com/morsdyce/ngReact-minesweeper",
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "angularjs-jest-serializer": "^0.3.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "json-loader": "0.5.2",
    "node-libs-browser": "0.5.2",
    "node-sass": "^4.5.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "15.4.2",
    "regenerator-runtime": "^0.11.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "angular": "1.4.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eventemitter2": "0.4.14",
    "flux": "2.1.1",
    "keymirror": "0.1.1",
    "lodash": "3.10.1",
    "object-assign": "4.0.1",
    "uuid": "3.1.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/jest/styleMock.js"
    },
    "transform": {
      "\\.js$": "babel-jest",
      "\\.html$": "<rootDir>/jest/htmlMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/fileMock.js"
    }
  }
}
